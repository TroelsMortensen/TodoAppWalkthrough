<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="448.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="todoapp.view.list.ListController">
   <children>
      <TableView fx:id="tableView" layoutX="-1.0" layoutY="-1.0" prefHeight="400.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="ownerColumn" prefWidth="150.0" text="Owner" />
          <TableColumn fx:id="textColumn" prefWidth="500.0" text="Text" />
          <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="409.0" mnemonicParsing="false" onAction="#onAddButton" text="Add todo" />
   </children>
</AnchorPane>
